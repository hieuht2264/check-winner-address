.row
  .col-12
    - if @result
      .row
        .col-md-5.offset-md-2
          %h4 Result
      .row
        .col-md-5.offset-md-2
          - @result.each do |address|
            %span=address
      .row
        .col-md-5.offset-md-2
          = link_to 'Back', root_path, class: 'btn btn-default'